      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="MoreSet">
      <EntityTypeMapping TypeName="IsTypeOf(Demo.More)">
        <MappingFragment StoreEntitySet="MoreSet">
          <ScalarProperty Name="ID" ColumnName="ID" />
          <ScalarProperty Name="Mail" ColumnName="Mail" />
