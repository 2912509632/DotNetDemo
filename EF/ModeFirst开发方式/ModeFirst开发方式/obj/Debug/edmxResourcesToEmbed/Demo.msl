      <EntityTypeMapping TypeName="IsTypeOf(Demo.Info)">
        <MappingFragment StoreEntitySet="InfoSet">
          <ScalarProperty Name="ID" ColumnName="ID" />
          <ScalarProperty Name="Name" ColumnName="Name" />
