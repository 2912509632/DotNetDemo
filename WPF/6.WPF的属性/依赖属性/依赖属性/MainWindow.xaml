<Window x:Class="依赖属性.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:依赖属性"
        mc:Ignorable="d"
        Title="依赖属性" Height="250" Width="325">
    <StackPanel Margin="10">
        <TextBox x:Name="txt_input" Margin="10" />
        <TextBox x:Name="txt_output" Margin="10"/>
        <Button x:Name="btn_test" Content="OK" Margin="10" Click="btn_test_Click" />
    </StackPanel>
</Window>
